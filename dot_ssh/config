Host *
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa
  SetEnv TERM=xterm-256color
  # Post-quantum key exchange with fallback to traditional algorithms
  # Servers without PQ support will automatically use the fallback algorithms
  KexAlgorithms sntrup761x25519-sha512@openssh.com,curve25519-sha256,curve25519-sha256@libssh.org,ecdh-sha2-nistp256,ecdh-sha2-nistp384,ecdh-sha2-nistp521,diffie-hellman-group-exchange-sha256

Include config.d/*

